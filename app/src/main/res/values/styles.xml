<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="colorPrimary">@color/c_medium</item>
        <item name="colorPrimaryDark">@color/c_dark</item>
        <item name="colorAccent">@color/c_accent</item>
        <item name="android:colorBackground">@color/c_light</item>
    </style>

    <style name="AppTheme.NoActionBar" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimaryDark">@color/c_dark</item>
        <item name="colorAccent">@color/c_accent</item>
        <item name="android:colorBackground">@color/c_light</item>
    </style>

</resources>
